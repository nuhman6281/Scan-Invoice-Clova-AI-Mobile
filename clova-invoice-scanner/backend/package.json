{
  "name": "clova-invoice-scanner-backend",
  "version": "1.0.0",
  "description": "CLOVA AI Invoice Scanner Backend API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "docker:build": "docker build -t clova-backend .",
    "docker:run": "docker run -p 3000:3000 clova-backend"
  },
  "keywords": [
    "clova",
    "ai",
    "invoice",
    "scanner",
    "ocr",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "CLOVA AI Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@types/form-data": "^2.2.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.3",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.0.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "sharp": "^0.32.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typedi": "^0.10.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.0.0",
    "@types/sharp": "^0.31.1",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/clova-invoice-scanner.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/clova-invoice-scanner/issues"
  },
  "homepage": "https://github.com/your-repo/clova-invoice-scanner#readme"
}
